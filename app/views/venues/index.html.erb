<p id="notice"><%= notice %></p>

<h1>Venues</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Box office fringe</th>
      <th>Box office opening</th>
      <th>Cafe description</th>
      <th>Code</th>
      <th>Email</th>
      <th>Fax</th>
      <th>Has bar</th>
      <th>Has booking over card</th>
      <th>Has booking over phone</th>
      <th>Has booking over web</th>
      <th>Has cafe</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Post code</th>
      <th>Web address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.address %></td>
        <td><%= venue.box_office_fringe %></td>
        <td><%= venue.box_office_opening %></td>
        <td><%= venue.cafe_description %></td>
        <td><%= venue.code %></td>
        <td><%= venue.email %></td>
        <td><%= venue.fax %></td>
        <td><%= venue.has_bar %></td>
        <td><%= venue.has_booking_over_card %></td>
        <td><%= venue.has_booking_over_phone %></td>
        <td><%= venue.has_booking_over_web %></td>
        <td><%= venue.has_cafe %></td>
        <td><%= venue.name %></td>
        <td><%= venue.phone %></td>
        <td><%= venue.lat %></td>
        <td><%= venue.lon %></td>
        <td><%= venue.post_code %></td>
        <td><%= venue.web_address %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
