<!DOCTYPE html>
<html>
  <head>
    <title>Fringe</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div class="row">
      <%= render "./layouts/bootstrap_nav_bar" %>
      <%= render "./home/navigation_bar" %>
      <%= render "./home/user_login_options" %>
      <!-- Devise wants some flash messages here -->
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>


      <%= yield %>
  </div>
  </div>
  </body>
</html>