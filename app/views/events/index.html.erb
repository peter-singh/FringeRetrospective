<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Age category</th>
      <th>Artist</th>
      <th>Code</th>
      <th>Description</th>
      <th>Festival</th>
      <th>Festival</th>
      <th>Genre</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Status</th>
      <th>Title</th>
      <th>Updated</th>
      <th>Url</th>
      <th>Venue</th>
      <th>Address</th>
      <th>Code</th>
      <th>Description</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Post code</th>
      <th>Website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.age_category %></td>
        <td><%= event.artist %></td>
        <td><%= event.code %></td>
        <td><%= event.description %></td>
        <td><%= event.festival %></td>
        <td><%= event.festival_id %></td>
        <td><%= event.genre %></td>
        <td><%= event.latitude %></td>
        <td><%= event.longitude %></td>
        <td><%= event.status %></td>
        <td><%= event.title %></td>
        <td><%= event.updated %></td>
        <td><%= event.url %></td>
        <td><%= event.venue %></td>
        <td><%= event.address %></td>
        <td><%= event.code %></td>
        <td><%= event.description %></td>
        <td><%= event.name %></td>
        <td><%= event.phone %></td>
        <td><%= event.post_code %></td>
        <td><%= event.website %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
