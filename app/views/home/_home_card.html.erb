<% @events.each do |event| %>
	<%= event.title %>
	<br>
	<%= event.festival %>
<!-- 	<%=  raw(event.description) %>
 -->	
 	<br>
 	<text>Reviews:</text>
 	<%= event.reviews.count %>
 	<br>

 	<% if user_signed_in? %>
 		<text>Signed in!</text>
 	<% else %>
 		<text> Not Signed In! </text>
 	<% end %>

 	<%= Emoji.find_by_alias("thumbsup").raw %>
 	<%= Emoji.find_by_alias("thumbsdown").raw %>
	<br>
 	<br>
<% end %>