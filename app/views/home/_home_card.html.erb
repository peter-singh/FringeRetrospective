<% @events.each do |event| %>

<%= react_component('EventCard', trace: nil,
props: {
	event: event.to_json,
 	venue: event.venue.to_json,
 	performances: event.performances.to_json,
 	userSignedIn: !current_user.nil?,
 	imageUrls: event.image_urls,
 	isFavourited: Event.favourited?(current_user, event.try(:id))
})
 %>

<% end %>

<%= paginate @events %>

<div class="container">
  <div class="row">
    <div id="image-div" class="col-xs-8 col-md-10" >
			<%= image_tag("cutout.png", :id => "footer-image") %>

    </div>
    <div class="col-xs-4 col-md-2" >
    	<span id="disclaimer-text">			<span> ðŸ‘‰ data provided courtesy of the </span><a href="https://api.edinburghfestivalcity.com/"> Edinburgh Festivals Listings API</a></span>
    </div>
  </div>
</div>