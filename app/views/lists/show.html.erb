<p id="notice"><%= notice %></p>

<h2><%= @list.name %></h2>

<%= react_component("List", props:{
    key: @list.id,
    listId: @list.id,
    listItems: @list.list_items,
    events: @list.events.map(&:to_json),
    userSignedIn: !current_user.nil?,
  }) %>

<div class=' col-xs-12'>
  <%= link_to 'Edit', edit_list_path(@list) %> |
  <%= link_to 'Back', lists_path %>
</div>