<p> <%= @results.count %> results found: </p>

<% @results.each do |r| %>
<form id="save_event_form" method="post" action="/events">

<%- model_class = Event -%>
<div class="page-header">
  <h1><%= r['title'] %></h1>
</div>

<dl class="dl-horizontal">

  <dt><strong><%= model_class.human_attribute_name(:age_category) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="age_category" value="<%= r['age_category'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:artist) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="artist" value="<%= r['artist'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="code" value="<%= r['code'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:festival) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="festival" value="<%= r['festival'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:festival_id) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="festival_id" value="<%= r['festival_id'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:genre) %>:</strong></dt>
  <dd><input readonly="readonly" size="50" type="text" name="genre" value="<%= r['genre'] %>"></dd>

  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
